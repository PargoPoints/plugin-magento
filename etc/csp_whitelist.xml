<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp/etc/csp_whitelist.xsd">
    <policies>
        <policy id="frame-src">
            <values>
                <value id="pargo" type="host">https://map.pargo.co.za/</value>
                <value id="pargo_staging" type="host">https://map.staging.pargo.co.za/</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="pargo-img-select" type="host">https://pargo.co.za</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
